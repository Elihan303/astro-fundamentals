---
interface Props {
  metaDesc?: string;
}

const { metaDesc } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content={metaDesc} />
    <title>Astro app</title>
  </head>
  <body>
    <slot />
  </body>
</html>
