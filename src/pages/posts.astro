---
import Layout from "../components/layout.astro";
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const data = await response.json();
---

<Layout>
  <h1>Publicaciones</h1>

  {
    data.map((e) => (
      <div>
        <h5>{e.id}</h5>
        <h3 class="title">{e.title}</h3>
        <p>{e.body}</p>
      </div>
    ))
  }

  <style>
    .title {
      color: skyblue;
    }
  </style>
</Layout>
